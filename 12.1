 internal class Calculator
 {
     private double a, b;
     public static double Add(double a, double b){return a + b;}
     public static double Subtraction(double a, double b) { return a - b; }
     public static double Multiplication(double a, double b) { return a * b;}
     public static double Division(double a, double b)
     {
         if (b == 0)
         {
             Console.WriteLine("Ошибка");
             return 0;
         }
         return a / b;
     }
     public static void Result()
     {
         Console.WriteLine($"Сложение {Add(5,9)}");
         Console.WriteLine($"Вычитание {Subtraction(7, 9)}");
         Console.WriteLine($"Умножение {Multiplication(3, 6)}");
         Console.WriteLine($"Деление {Division(15, 5)}");
     }
 }
 class Program
{
    static void Main(string[] args)
    {
        Calculator.Result();
    }
}
